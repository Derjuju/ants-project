{
    "name": "derjuju/ants-project",
    "description": "Simulateur",
    "keywords": ["ia", "machine learning", "php-ml", "phaser"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "predis/predis": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "^2.3",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "AntsProject\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "secure-http":false,
      "sort-packages": true
    }
}
